<!-- Wrapper -->
<!-- Scripts -->
<script src="assets/js/jquery.min.js"></script>
<div id="wrapper" class="divided">
    <!-- One -->
    <section class="section-intro section-one">
        <div class="parallax-window">
            <div class="parallax">
                <div class="intro-text">
                    <h1>Shezad & Natasha</h1>
                    <h3>7 January, Bombay</h3>
                </div>
                <div class="intro-rsvp">
                    <span class="rsvp-button">RSVP</span>
                </div>
                <div class="intro-img-one intro-img"></div>
                <div class="intro-img-two intro-img"></div>
            </div>
            <script>
                // set window height to cover viewport across devices
                $('.intro-img').css('height', $(window).height());
            </script>
        </div>
    </section>

    <section class="first-block-text section-two">
        <div class="spotlight style1">
            <div class="intro-content content content-align-center orient-center">
                <h2 class="content-header">MR &amp; MRS</h2>
                <p>We are elated to invite you to our wedding and request the pleasure of your company, as we join together in marriage.</p>
                <p>We hope you are able to join us in Mumbai, India for our wedding celebrations.
                      Please take a moment to explore our website for further details.</p>
            </div>
        </div>
    </section>

    <!-- Three -->
    <section class="spotlight style1 orient-right content-align-left image-position-center section-three" id="first">
        <div class="content">
            <h2 class="content-header">Save The Date</h2>
            <p>Our wedding will take place on Sunday the 7th of January, 2018.</p>
            <p>We look forward to your choicest blessings and celebrating with you on this auspicious occasion.</p>
        </div>
        <div class="image">
            <img src="images/spotlight01.jpg" alt="" />
        </div>
    </section>

    <!-- Four -->
    <section class="spotlight style1 orient-left content-align-left image-position-center section-four">
        <div class="image mobile">
            <img src="images/diagonal-left.jpg" alt="" />
        </div>
        <div class="content city-dreams-content">
            <h2 class="content-header">Welcome to the “city of dreams”</h2>
            <p>DISCOVER<br />
                As well as attending our wedding celebration, we want you to also experience and explore the magic that is Mumbai.
                Whether it is a trip to the historic, Gateway of India or dinner at the Taj; there is so much to see and do.</p>
        </div>
        <div class="image mobile">
            <img src="images/diagonal-right.jpg" alt="" />
        </div>
        <div class="image desktop city-dreams-image">
            <img class="diagonal-images" src="images/diagonal-images.jpg" alt="" />
        </div>
    </section>

    <!-- Five -->
    <section class="adress-block section-five">
        <div class="spotlight style1">
            <div class="intro-content content content-align-center orient-center">
                <h2 class="content-header">Our Wedding</h2>
                <p>
                    Our wedding will be a traditional Parsi ceremony, which will involve age-old rituals.
                    7pm onwards, we look forward to you celebrating our important day with us.
                </p>
                <p>
                    Location:<br />
                    Seth Jeejeebhoy Dadabhoy Agiary Baug<br />
                    <b><i>
                        Off Pilot Bunder Road, Near Afghan Church<br />
                        Colaba, Mumbai – 400 005
                    </i></b>
                </p>
            </div>
        </div>
    </section>

    <!-- Six -->
    <section class="adress-block section-five">
        <div class="spotlight style1">
            <div class="intro-content content content-align-center orient-center">
                <h2 class="content-header">Our Love Story &nbsp;<span style="color:red">&#9829;</span></h2>
                <p>
                    <b>March 2015</b><br />
                    First met in Dubai
                </p>
                <p>
                    <b>May 2015</b><br />
                    First date in Dubai
                </p>
                <p>
                    <b>June 2015 - May 2017</b><br />
                    Making memories all around the world
                </p>
                <p>
                    <b>May 2017</b><br />
                    He proposed. She said yes
                </p>
                <p>
                    <b>January 2018</b><br />
                    Our Happily ever after begins &#9829;
                </p>
            </div>
        </div>
    </section>

    <!-- Seven -->
    <section class="spotlight style1 orient-left content-align-left image-position-center section-six">
        <div class="rsvp-block content">
            <div class="inner medium">
                <h2 class="rsvp-header">We would love you to join us for our big day</h2>
                <form id="rsvp-form" method="post">
                    <h3>RSVP</h3>
                    <div class="first-name">
                        <input type="text" name="name" placeholder="First and Last Name" id="name" value="" required />
                    </div>

                    <div class="rsvp-radio-container">
                        <div class="field third first rsvp-radio">
                            <input type="radio" id="rsvp-yes" value="Yes" name="rsvp" checked />
                            <label for="rsvp-yes">Yes</label>
                        </div>
                        <div class="field third rsvp-radio">
                            <input type="radio" id="rsvp-no" value="No" name="rsvp" />
                            <label for="rsvp-no">No</label>
                        </div>
                    </div>

                    <div class="meal-pref">
                        <div class="select-container">
                            <select class="meal-pref-select nonVegetarian" name="nonVegetarian" id="nonVegetarian">
                                <option value="0">0</option>
                                <option selected="selected" value="1">1</option>
                                <option value="2">2</option>
                                <option value="3">3</option>
                                <option value="4">4</option>
                                <option value="5">5</option>
                                <option value="6">6</option>
                                <option value="7">7</option>
                                <option value="8">8</option>
                                <option value="9">9</option>
                            </select>
                            <span class="nonVegetarian meal-label">Non Vegetarian</span>
                            <i class="select-down-icon fa fa-chevron-down"></i>
                        </div>
                        <div class="select-container">
                            <select class="meal-pref-select vegetarian" name="vegetarian" id="vegetarian">
                                <option value="0">0</option>
                                <option value="1">1</option>
                                <option value="2">2</option>
                                <option value="3">3</option>
                                <option value="4">4</option>
                                <option value="5">5</option>
                                <option value="6">6</option>
                                <option value="7">7</option>
                                <option value="8">8</option>
                                <option value="9">9</option>
                            </select>
                            <span class="vegetarian meal-label">Vegetarian</span>
                            <i class="select-down-icon fa fa-chevron-down"></i>
                        </div>
                    </div>

                    <ul class="actions">
                        <button class="submit-button" type="submit" name="submit" id="submit" />
                        <span class="submit-text">Confirm</span>
                        <img class="submit-loader hidden" src="assets/pie.svg" />
                        <img class="submit-success hidden" src="assets/tick.png" />
                    </ul>
                </form>
            </div>
        </div>
        <div class="image">
            <img class="diagonal-left" src="images/rsvp-right.jpg" alt="" />
        </div>
    </section>

    <!-- Footer -->
    <footer class="site-footer align-center">
        <div class="inner">
            <div class="photos-courtesy">
                Photos courtesy - The Wedding Pixels
            </div>
            <span><b>Shezad & Natasha</b></span>
        </div>
    </footer>
</div>
